{
    "compilerOptions": {
        "target": "ES2022",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "resolveJsonModule": true,
        "paths":{
            "@utilize": ["src/utils"],
            "@typings": ["src/typings"],
            "@command": ["src/commands"]
        },
        "module": "commonjs"
    },
    "exclude": [
        "**/node_modules",
        "config.example.ts",
        "**/dist"
    ]
}